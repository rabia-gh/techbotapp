node {
        stage('Checkout SCM'){
                git branch: 'master', url: 'https://github.com/rabia-gh/techbotapp.git'
        }

        stage('Install node modules'){
               bash "npm install"
        }
        stage('Build'){
                bash "cd front"
                bash "npm install @angular/compiler-cli"
                bash "npm install -g @angular/cli"
        }
        stage('Deploy'){
                bash "ng serve " 

        }
}
